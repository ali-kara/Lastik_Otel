<Window x:Class="Lastik_Otel.Yardim.Versiyonlar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Height="700" Width="500" 
        WindowStyle="None" Background="{x:Null}"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize" 
        AllowsTransparency="True" MouseDown="Window_MouseDown">

    <Grid >
        <Border Background="#43FB84" CornerRadius="25,25,25,25">
            <Grid Margin="20,20,20,20">
                <TextBox Name="txtVersiyon" VerticalAlignment="Top" Height="596" FontSize="20" IsReadOnly="True" Background="#FF94F07A" />
                <Button Name="btnCikis" Style="{DynamicResource RoundedButtonKayitIcin}" Click="btnCikis_Click" VerticalAlignment="Bottom" Height="50" Margin="128,0,127,0" Content="ÇIKIŞ"/>
            </Grid>
        </Border>
    </Grid>
</Window>
